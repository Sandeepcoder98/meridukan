<!-- Preloader -->
<div id="preloader">
  <div id="status"><i class="fa fa-spinner fa-spin"></i></div>
</div>
<section>
  <div class="mainpanel">
    <div class="pageheader">
      <h2><i class="fa fa-home"></i> Dashboard <span>Subtitle goes here...</span></h2>
      <div class="breadcrumb-wrapper">
        <span class="label">You are here:</span>
        <ol class="breadcrumb">
          <li><a href="index.html">Home</a></li>
          <li class="active">Dashboard</li>
        </ol>
      </div>
    </div>
    <div class="hovercard" id="seller-profile">
      <div class="cardheader">  <%= image_tag asset_path(current_user.store.cover.url), :width=>"100%", :height=>"175px;"%></div>
      <div class="avatar"> <%= image_tag asset_path(current_user.avatar.url) %></div>
      <ol class="shop-list">
        <li class="item first">
          <div class="product-shop">
            <h2 class="product-name"><%= current_user.try(:first_name).try(:camelize)  %></h2>
            <div class="lead">
              <div class="starrr" id="stars"><span class="glyphicon .glyphicon-star-empty glyphicon-star-empty"></span><span class="glyphicon .glyphicon-star-empty glyphicon-star-empty"></span><span class="glyphicon .glyphicon-star-empty glyphicon-star-empty"></span><span class="glyphicon .glyphicon-star-empty glyphicon-star-empty"></span><span class="glyphicon .glyphicon-star-empty glyphicon-star-empty"></span></div>
            </div>
            <div class="desc std">
              <ul class="social-network social-circle">
                <li><a title="Rss" class="icoRss" href="#"><i class="fa fa-rss"></i></a></li>
                <li><a title="Facebook" class="icoFacebook" href="#"><i class="fa fa-facebook"></i></a></li>
                <li><a title="Twitter" class="icoTwitter" href="#"><i class="fa fa-twitter"></i></a></li>
                <li><a title="Google +" class="icoGoogle" href="#"><i class="fa fa-google-plus"></i></a></li>
                <li><a title="Linkedin" class="icoLinkedin" href="#"><i class="fa fa-linkedin"></i></a></li>
              </ul>
            </div>
          </div>
        </li>
      </ol>
    </div>
    <div class="contentpanel">
      <div class="row">
        <div class="col-sm-6 col-md-3">
          <div class="panel  panel-stat">
            <div class="panel-heading">
              <div class="stat">
                <div class="row">
                  <div class="col-xs-12">
                    <small class="stat-label">Sales in queue</small>
                    <h1>900k+</h1>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="col-sm-6 col-md-3">
          <div class="panel panel-stat">
            <div class="panel-heading">
              <div class="stat">
                <div class="row">
                  <div class="col-xs-12">
                    <small class="stat-label">Earned this month</small>
                    <h1>54.40%</h1>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="col-sm-6 col-md-3">
          <div class="panel panel-stat">
            <div class="panel-heading">
              <div class="stat">
                <div class="row">
                  <div class="col-xs-12">
                    <small class="stat-label">Open purchases</small>
                    <h1>300k+</h1>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="col-sm-6 col-md-3">
          <div class="panel panel-stat">
            <div class="panel-heading">
              <div class="stat">
                <div class="row">
                  <div class="col-xs-12">
                    <small class="stat-label">Balance</small>
                    <h1>$655</h1>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-sm-8 col-md-9">
          <div class="panel panel-default">
            <div class="panel-body">
              <div class="row">
                <div class="col-sm-8">
                  <h5 class="subtitle mb5">Network Performance</h5>
                  <p class="mb15">Duis autem vel eum iriure dolor in hendrerit in vulputate...</p>
                  <div id="basicflot" style="width: 100%; height: 300px; margin-bottom: 20px"></div>
                </div>
                <div class="col-sm-4">
                  <h5 class="subtitle mb5">Server Status</h5>
                  <p class="mb15">Summary of the status of your server.</p>
                  <span class="sublabel">CPU Usage (40.05 - 32 cpus)</span>
                  <div class="progress progress-sm">
                    <div style="width: 40%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="40" role="progressbar" class="progress-bar progress-bar-primary"></div>
                  </div>
                  <span class="sublabel">Memory Usage (32.2%)</span>
                  <div class="progress progress-sm">
                    <div style="width: 32%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="40" role="progressbar" class="progress-bar progress-bar-success"></div>
                  </div>
                  <span class="sublabel">Disk Usage (82.2%)</span>
                  <div class="progress progress-sm">
                    <div style="width: 82%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="40" role="progressbar" class="progress-bar progress-bar-danger"></div>
                  </div>
                  <span class="sublabel">Databases (63/100)</span>
                  <div class="progress progress-sm">
                    <div style="width: 63%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="40" role="progressbar" class="progress-bar progress-bar-warning"></div>
                  </div>
                  <span class="sublabel">Domains (2/10)</span>
                  <div class="progress progress-sm">
                    <div style="width: 20%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="40" role="progressbar" class="progress-bar progress-bar-success"></div>
                  </div>
                  <span class="sublabel">Email Account (13/50)</span>
                  <div class="progress progress-sm">
                    <div style="width: 26%" aria-valuemax="100" aria-valuemin="0" aria-valuenow="40" role="progressbar" class="progress-bar progress-bar-success"></div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="col-sm-4 col-md-3">
          <div class="panel panel-default">
            <div class="panel-body">
              <h5 class="subtitle mb5">Most Browser Used</h5>
              <p class="mb15">Duis autem vel eum iriure dolor in hendrerit in vulputate...</p>
              <div id="donut-chart2" class="ex-donut-chart"></div>
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="people-list">
          <% current_user.products.first(3).each do |product|%>
            <div class="col-md-4">
              <div class="results-list people-item">
                <div class="media">
                  <a class="pull-left" href="#">
                  <%= image_tag asset_path(product.try(:galleries).try(:first).try(:photo).try(:url)),:class=>"", :size=>"70x70"%>
                  </a>
                  <div class="media-body">
                    <h4 class="filename"><%= product.title%></h4>
                    <small class="text-muted">Created: <%= product.created_at.strftime("%B %d, %Y at %I %p")%></small><br>
                    <small class="text-muted">Created: <%= product.created_at%></small><br>
                    <small class="text-muted">Modified: <%= product.updated_at.strftime("%B %d, %Y at %I %p")%></small>
                  </div>
                </div>
              </div>
            </div>
          <% end %>
          <%= button_to "Show All Products" , products_path, class: "btn btn-danger pull-right", method: :get %>
        </div>

      </div>
    </div>
  </div>
</section>
<script>
  jQuery(document).ready(function(){     
     "use strict"
     jQuery('.ckbox input').click(function(){
         var t = jQuery(this);
         if(t.is(':checked')){
             t.closest('tr').addClass('selected');
         } else {
             t.closest('tr').removeClass('selected');
         }
     });
     jQuery('.star').click(function(){
         if(!jQuery(this).hasClass('star-checked')) {
             jQuery(this).addClass('star-checked');
         }
         else
             jQuery(this).removeClass('star-checked');
         return false;
     });
     jQuery('.table-email .media').click(function(){
         location.href="read.html";
     });
     
  });
</script>