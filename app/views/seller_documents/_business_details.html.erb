<%= form_for (@seller_document) do |f|%>
	<div class="form-group">
		<%= f.label :business_name,"Business Name"%>
		<%= f.text_field :business_name,class: "form-control"%>
	</div>
	<div class="form-group">
		<%= f.label :business_type,"Business Type"%>
		<%= f.select :business_type,SellerDocument::BUSINESS_TYPES,{prompt: "Select"},class: "form-control"%>
	</div>
	<div class="form-group">
		<%= f.label :pan_card_number,"Personal PAN"%>
		<%= f.text_field :pan_card_number,class: "form-control"%>
	</div>
	<div class="form-group">
		<%= f.file_field :pan_card,class: "form-control"%>
	</div>
	<div class="form-group">
		<%= f.label :vat_card_number,"TIN/VAT"%>
		<%= f.text_field :vat_card_number,class: "form-control"%>
	</div>
	<div class="form-group">
		<%= f.file_field :vat_card,class: "form-control"%>
	</div>
	<div class="form-group">
		<%= f.label :tan_card_number,"TAN"%>
		<%= f.text_field :tan_card_number,class: "form-control"%>
	</div>
	<div class="form-group">
		<%= f.file_field :tan_card,class: "form-control"%>
	</div>
    <div class="form-group">
		<%= f.submit "Save"%>
	</div>
<%end%>

