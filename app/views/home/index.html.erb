<div class="mm-toggle-wrap">
  <div class="mm-toggle">
    <i class="icon-align-justify"></i>
    <span class="mm-label">Menu</span> 
  </div>
</div>
<div class="span_1_of_1">
  <h2>Select your Product Categories <br>
    Find it on here
  </h2>
  <%= form_tag search_index_path, method: :get do %>
    <div class="search">
      <div class="input-group">
          <div class="input-group-btn search-panel">
            <button type="button" class="btn dropdown-toggle select-option" data-toggle="dropdown">
              <span id="search_concept">Products</span> <span class="caret"></span>
            </button>
            <ul id="search_selection" class="dropdown-menu" role="menu">
              <li><a data-action="/search" href="#">Products</a></li>
              <li><a data-action="/search/stores" href="#">Shops</a></li> 
            </ul>
          </div>
          <input type="text" class="form-control" name="q" placeholder="Search" id="search_text">
          <span class="input-group-btn">
            <button class="btn btn-default" type="button" id="search_button"><span class="fa fa-search"></span></button>
          </span>
      </div>
    </div>
  <% end %>
</div>
